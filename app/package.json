		"@capacitor/dialog": "^4.0.1",
		"@capacitor/filesystem": "^4.1.2",
		"@capacitor/ios": "4.3.0",
		"@capacitor/keyboard": "^4.0.1",
		"@capacitor/preferences": "^4.0.1",
		"@ionic/pwa-elements": "^3.1.1",
		"@ionic/vue": "^6.3.1",
		"@ionic/vue-router": "^6.3.1",
		"@vueuse/core": "^9.3.0",
		"@vueuse/head": "^0.9.8",
		"@vueuse/integrations": "^9.3.0",
		"@vueuse/math": "^9.3.0",
		"@vueuse/router": "^9.3.0",
		"axios": "^1.1.3",
