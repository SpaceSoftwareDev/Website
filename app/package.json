{
	"name": "starter",
	"version": "0.0.1",
	"private": true,
	"description": "Fun mobile app platform for the meme lovers",
	"scripts": {
		"dev": "vite",
		"lint": "eslint . --ext .vue,.ts --ignore-path .gitignore",
		"build": "vite build",
		"serve": "vite preview",
		"ionic:serve": "yarn dev",
		"native:sync": "cap sync",
		"native:update": "yarn build && yarn native:sync",
		"preview:ios": "yarn build && ionic cap run ios --no-build --external",
		"preview:android": "yarn build && ionic cap run android --livereload --external",
		"upgrade": "yarn upgrade-interactive"
	},
	"dependencies": {
		"@capacitor/action-sheet": "^4.0.1",
